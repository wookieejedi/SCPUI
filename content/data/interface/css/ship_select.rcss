	width: 5%;
	float: left;
	padding-top: 20%;
	padding-right: 5%;
}

.icon
{
	width: 50%;
	float: right;
	z-index: 7;
}

.shiplist_element
{
	display: block;
	color: #7b7b7b;
}
.shiplist_element:hover
{
	color: #abbbab;
}
.shiplist_element:checked
{
	color: #6f92f5;
}

.info
{
	color: green;
	padding-left: 5;
}

div#wings_wrapper
{
	position: absolute;
	left: 28%;
	bottom: 5%;
	width: 50%;
	height: 20%;
}

.wing
{
	width: 20%;
	float: left;
	margin-right: 3%;
}

.slot_wrapper
{
	height: 85%;
}

.wing_name
{
	text-align: center;
	margin-top: 7%;
}

.wing_slot
{
	width:31%;
	height:31%;
}

/*This is dumb.. but I want to hide the callsign during ship dragging, but not during player dragging*/
/*and getting the dragged element via lua is buggy...*/

div:drag
{
	color: rgba(0,0,0,0);
}

#callsign_drag:drag
{
	color: rgba(255, 255, 255, 255);
}

.wing_slot img
{
	width: 100%;
	height: 100%;
}

.wing_slot ani
{
	width: 100%;
	height: 100%;
}

.wing_one
{
	margin-left: 33.5%;
	margin-right: 33.5%;
}

.wing_one:drag
{
	margin-left: 0;
	margin-right: 0;
}

.wing_two
{
	float: left;
}

.wing_three
{
	float: right;
}

.wing_four
{
	margin-top: 32.5%;
	margin-left: 33.5%;
	margin-right: 33.5%;
}

.slot_name
{
	text-align: center;
	font-size: 0.8em;
}

.available
{
	cborder-decorator: corner-borders;
    cborder-thickness: 0.6%;
	cborder-color: teal;
}

.available:valid
{
	cborder-decorator: corner-borders;
    cborder-thickness: 0.6%;
	cborder-color: white;
}

.locked
{
	cborder-decorator: corner-borders;
    cborder-thickness: 0.6%;
	cborder-color: grey;
}

div#breakout_button
{
	position: absolute;
	bottom: -11.5%;
	right: 5%;
