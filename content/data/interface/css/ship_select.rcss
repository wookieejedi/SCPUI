	icon-image: arrow-wide-down.png;
}

#ship_icon_list scrollbarvertical sliderarrowdec:hover
{
	icon-image: arrow-wide-up-h.png;
}

#ship_icon_list scrollbarvertical sliderarrowinc:hover
{
	icon-image: arrow-wide-down-h.png;
}

#ship_icon_list scrollbarvertical sliderarrowdec:active
{
	icon-image: arrow-wide-up-o.png;
}

#ship_icon_list scrollbarvertical sliderarrowinc:active
